{
  "name": "n8n-nodes-whatsapp-green-api",
  "version": "0.1.5",
  "description": "n8n node to send WhatsApp messages via Green API",
  "keywords": [
    "n8n-community-node-package",
    "whatsapp",
    "green-api",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://green-api.com",
  "author": {
    "name": "Ido",
    "email": "ido.mations1@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idobe977/n8n-nodes-green-api.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GreenApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GreenApi/GreenApi.node.js"
    ]
  },
  "devDependencies": {
    "@types/request": "^2.48.12",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-core": "^1.14.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "request": "^2.88.2"
  }
}
